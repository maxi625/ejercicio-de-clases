class Rectangulo:
 
     def __init__(self, longitud, anchura):
 
         self.longitud = longitud
 
         self.anchura = anchura
 
 
     def area(self):
 
         return self.longitud * self.anchura
 
 
     def perimetro(self):
 
         return 2 * (self.longitud + self.anchura)
 
 rectangulo1 = Rectangulo(5, 3)
 
 print(f"Área del rectángulo: {rectangulo1.area()}")  
 
 print(f"Perímetro del rectángulo: {rectangulo1.perimetro()}")  
