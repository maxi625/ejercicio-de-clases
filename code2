from datetime import datetime
 
 class Coche:
     def __init__(self, marca, modelo, año_fabricacion):
         self.marca = marca
         self.modelo = modelo
         self.año_fabricacion = año_fabricacion
     
     def obtener_edad(self):
         
         año_actual = datetime.now().year
         
         return año_actual - self.año_fabricacion
 
 
 coche1 = Coche("ford", "fiesta", 2015)
 print(f"El coche {coche1.marca} {coche1.modelo} tiene {coche1.obtener_edad()} años.")
